    "def f(x):\n",
    "    n = 0\n",
    "    if x < 0:\n",
    "        while n >= x:\n",
    "            n = n - 1\n",
    "        return n + 1\n",
    "    else:\n",
    "        while n <= x:\n",
    "            n = n + 1\n",
    "        return n - 1"
   ]
